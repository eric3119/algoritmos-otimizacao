@article{Defersha2006,
title = "A comprehensive mathematical model for the design of cellular manufacturing systems ",
journal = "International Journal of Production Economics ",
volume = "103",
number = "2",
pages = "767 - 783",
year = "2006",
note = "",
issn = "0925-5273",
doi = "http://dx.doi.org/10.1016/j.ijpe.2005.10.008",
url = "http://www.sciencedirect.com/science/article/pii/S0925527306000995",
author = "Fantahun M. Defersha and Mingyuan Chen",
keywords = "Cellular manufacturing",
keywords = "Dynamic cell configuration",
keywords = "Alternate routings",
keywords = "Lot splitting",
keywords = "Workload balancing",
keywords = "Integer programming ",
abstract = "The design of cellular manufacturing systems (CMS) involves many structural and operational issues. One of the important design steps is the formation of part families and machine cells. In this paper, a comprehensive mathematical model for the design of \{CMS\} based on tooling requirements of the parts and tooling available on the machines is proposed. The model incorporates dynamic cell configuration, alternative routings, lot splitting, sequence of operations, multiple units of identical machines, machine capacity, workload balancing among cells, operation cost, cost of subcontracting part processing, tool consumption cost, setup cost, cell size limits, and machine adjacency constraints. Numerical examples are presented to demonstrate the model and its potential benefits. "
}


