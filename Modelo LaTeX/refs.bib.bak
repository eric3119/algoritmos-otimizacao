@ARTICLE{Choobineh1988,
  author = {Choobineh, F.},
  title = {A framework for the design of cellular manufacturing systems},
  journal = {International Journal of Production Research},
  year = {1988},
  volume = {26},
  pages = {1161-1172},
  number = {7},
  abstract = { Abstract A two-stage procedure for the design of a cellular manufacturing
	system is proposed. The first stage forms the part families. The
	use of clustering techniques with a new proximity measure is advocated
	for this stage. The proximity measure uses the manufacturing operations
	and the operations' sequences. The second stage forms the machine
	cells. An integer programming model is proposed for this stage. The
	solution of this model will specify the type and the number of machines
	in each cell and the assignment of the part families to the cells.
	The relevance of this approach in the design of flexible manufacturing
	systems is discussed. },
  doi = {10.1080/00207548808947932},
  url = {http://www.tandfonline.com/doi/abs/10.1080/00207548808947932}
}

@ARTICLE{Chu1989,
  author = {Chu, C. H.},
  title = {Cluster analysis in manufacturing cellular formation},
  journal = {Omega},
  year = {1989},
  volume = {17},
  pages = {289--295},
  number = {3},
  abstract = {Cluster analysis, an analytical method used for classifying objects,
	has been developed over the last century, and the literature on cluster
	analysis has exploded during the past two decades. This technique
	has been broadly used in the fields of biology, social science, and
	psychology, but only very little in manufacturing. In this paper,
